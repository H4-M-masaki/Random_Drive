


<h>ユーザー情報編集</h>

<h4>会員ID</h4>
<%= @user.id %>

<%= form_with model: @user, url: admin_user_path(@user.id),  local:true do |f| %>
<h4>ユーザーネーム</h4>
<%= f.text_field :name %>
<h4>メールアドレス</h4>
<%= f.email_field :email %>
<h4>会員ステータス</h4>
<%= f.radio_button :is_deleted, :true %>退会
<%= f.radio_button :is_deleted, :false %>有効

<%= f.submit "編集内容を保存" %>
<% end %>